<?xml version="1.0" encoding="utf-8"?>
<com.lovejjfg.blogdemo.ui.SlideToFinishLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#ebf5f5"
        android:orientation="vertical">

        <include layout="@layout/app_bar" />



            <com.lovejjfg.blogdemo.ui.ClosableSlidingLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <WebView
                    android:id="@+id/web"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>

            </com.lovejjfg.blogdemo.ui.ClosableSlidingLayout>


    </LinearLayout>


</com.lovejjfg.blogdemo.ui.SlideToFinishLayout>

